{
    "extends": [
        "plugin:@wordpress/eslint-plugin/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "wp": true,
        "wpApiSettings": true,
        "wcSettings": true,
        "es6": true,
        "jQuery": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "modules": true,
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "overrides": [
        {
          "files": ["resources/js/blocks/**/*.js"],
          "rules": {
            "@wordpress/no-unsafe-wp-apis": "off"
          }
        }
      ]
}